   	</p:dialog> 
        <p:dialog widgetVar="sessionDialog" header="Please sign in again" appendTo="@(body)" width="50%" height="200px" modal="true" draggable="false" resizable="false" closable="false">
  <h:form>
  <div style="height:20px;"></div>
   <center>
   <h:outputText value="You were signed out of your account." style="font-size:20px;font-family:'Arial.san-serif';font-weight:bold"></h:outputText> 
  <div style="height:20px"></div>
 <h:outputText value="Please press 'Reload' to sign in to the Console again" style="font-size:20px;font-family:'Arial.san-serif';font-weight:bold"></h:outputText>
  <div style="height:50px;"></div><p:commandButton value="Reload" action="login2.xhtml?faces-redirect=true" update="@form" process="@this"></p:commandButton>
  </center>
   </h:form>
   </p:dialog>
    <script>
				$(window).load(function() {
					
					$('#overlay').fadeOut(2000, function() {
					$('#center_content').fadeIn(500);
						$('.layout-main').fadeIn(500);
					});
				});
			
			</script>

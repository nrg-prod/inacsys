	<div id="overlay">
						<p:graphicImage value="../../images/inacsyspanelimage.gif" style="width:30%;height:60%; display: block;margin-left: auto;margin-right: auto;"></p:graphicImage>
						<!-- <div class="mouse"></div>
						<p>Loading</p> -->
					</div>
	<div class="layout-main" style="background-color:white;display:none;overflow-x: auto;height:100%" id="contentDiv">
                <div class="layout-main-content">
         <h:form  id="center_content">
					<p:remoteCommand name="remoteDialog" action="#{vendorRegisterFormMB.hideDialogBox}"	update=":center_content"></p:remoteCommand>
					<p:remoteCommand name="remoteDialog1" action="#{productRegisterFormMB.hideDialogBox}" update=":center_content"></p:remoteCommand>
					<p:remoteCommand name="remoteDialog2" update=":center_content"></p:remoteCommand>
					<ui:insert name="content">
					</ui:insert>
				</h:form>
                </div>

        <body class="main-body">
       <div class="layout-wrapper menu-layout-static"> 
         <div class="topbar clearfix" style="height:10%">
        <div class="topbar-left">            
            <h:form>
            <p:commandLink ajax="false" action="#{loginMB.goHome}" style="text-decoration: none !important;">
             <!-- <div class="logo">
              </div> -->
              <h:outputText value="INACSYS" style="font-size:30px;color:#fff;font-weight:bold"></h:outputText>
            </p:commandLink>
            </h:form>
           
        </div>
        
        <div class="topbar-right">
            <a id="menu-button" href="" style="color:white">
                <i class="fa fa-bars"></i>
            </a>
            
            <a id="topbar-menu-button" href="" style="color:white;font-size:25px;margin-top: 0px;">
                <i class="fa fa-user"></i>
                
            </a>
            
            
            <ul class="topbar-items fadeInDown animated">
             <li>
                  <h:form><p:commandLink action="login.xhtml?faces-redirect=true"> <i class="fa fa-power-off" style="color:white;font-size:25px;margin-top: 5px;">
                   </i></p:commandLink></h:form>
                  </li>
                <li>
                     <a href="">
                        <i class="topbar-icon fa fa-fw fa-user" style="color:white;font-size:25px;margin-top: 5px;"></i>
                    </a> 
                    <ul class="poseidon-menu animated">
                    
                        <li role="menuitem" class="clientlid">
                           
                                <span >Client ID</span>&nbsp;&nbsp;&nbsp;&nbsp;<span>:</span>&nbsp;&nbsp;&nbsp;<h:outputText value="#{loginMB.clientName}" ></h:outputText>
                          
                        </li>
                        <li role="menuitem" class="clientlid">
                          
                                <span>User Name</span>&nbsp;&nbsp;<span>:</span>&nbsp;&nbsp;&nbsp;<h:outputText value="#{loginMB.userName}" ></h:outputText>
                            
                        </li>
                        <li role="menuitem" class="clientlid">
                          
                                <span>User Type</span>&nbsp;&nbsp;&nbsp;&nbsp;<span>:</span>&nbsp;&nbsp;&nbsp;<h:outputText value="#{loginMB.userType}" ></h:outputText>
                           
                        </li>
                       
                    </ul>
                </li>
                 <li style="margin-top: 4px;">
                 <h:form id="globasearchlform">
                       <!-- <p:inputText value="#{loginMB.purchaseorder.golbalnamesearch}" placeholder="Search"
											style="font-size:12px"></p:inputText> -->
											<p:autoComplete id="autocomplete" completeMethod="#{loginMB.globalsearchValue}"
              scrollHeight="250" itemLabel="#{glo.modulevalue}" itemValue="#{glo.modulevalue}/#{glo.moduleid}/#{glo.approvalStatus}" var="glo">
              <p:ajax event="itemSelect" listener="#{loginMB.selectListener}"></p:ajax>
              </p:autoComplete>&nbsp;
									
												
									<!-- 	<p:commandButton icon="fa fa-search" value="Go"
											onclick="globalBlockUI();"></p:commandButton> -->
										 <p:remoteCommand name="globalsearchblockUI" action="#{loginMB.globalSearch}"></p:remoteCommand>
				 </h:form>
                  </li>
                 
            </ul> 
        </div>
    </div>
    <h:form id="menuform">
        <div class="layout-menu-container">
                   <div class="search-input">
                   <p:autoComplete completeMethod="#{loginMB.globalsearchValue}" ></p:autoComplete>
                       <!-- <p:inputText value="#{loginMB.purchaseorder.golbalnamesearch}" placeholder="Search"
											style="font-size:12px" type="input"></p:inputText> --> &nbsp;
										<p:commandButton icon="fa fa-search" value="Go"
											onclick="globalBlockUI();"></p:commandButton>
											 <p:remoteCommand name="globalsearchblockUI" action="#{loginMB.globalSearch}"></p:remoteCommand>
                    </div> 
               
        
        <ui:include src="../../pages/xhtml/menu.xhtml" />
